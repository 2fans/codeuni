import telnetlib

hostname = '192.168.56.101'
username = 'tufan'
password = '123'

connection = telnetlib.Telnet(hostname)
connection.read_until(b'Username: ')
connection.write(username.encode('ascii') + b'\n')
connection.read_until(b'Password: ')
connection.write(password.encode('ascii') + b'\n')

connection.write(b'show version\n')
output = connection.read_all().decode('ascii')
print(output)

connection.close()
